<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/icono_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Urban_Shop"
        tools:targetApi="31">
        <activity
            android:name=".ropa"
            android:exported="false"
            android:parentActivityName=".MainActivity2"/>
        <activity
            android:name=".SplashScreen"
            android:exported="true"
            android:theme="@style/Theme.Urban_Shop1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivityman"
            android:exported="false"
            android:parentActivityName=".Man" />
        <activity
            android:name=".Man"
            android:exported="false"
            android:parentActivityName=".MainActivity2" />
        <activity
            android:name=".Woman"
            android:exported="false"
            android:parentActivityName=".MainActivity2" />

        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".MainActivity2" />

        <activity
            android:name=".MainActivitywoman"
            android:exported="false"
            android:parentActivityName=".Woman" />
        <activity
            android:name=".MainActivity2"
            android:exported="true"></activity>
    </application>

</manifest>